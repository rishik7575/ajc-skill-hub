# Production Environment Variables
# Copy this file to .env.local and update with your production values

# Application
VITE_APP_NAME=AJC Skill Hub
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Professional skill development platform with industry-focused internship programs

# URLs
VITE_API_URL=https://api.ajc-skill-hub.com
VITE_WEBSITE_URL=https://ajc-skill-hub.com

# Features
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Analytics
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
VITE_GOOGLE_TAG_MANAGER_ID=GTM-XXXXXXX

# Error Reporting
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Social Media
VITE_LINKEDIN_URL=https://linkedin.com/company/ajc-skill-hub
VITE_TWITTER_URL=https://twitter.com/ajcskillhub
VITE_FACEBOOK_URL=https://facebook.com/ajcskillhub
VITE_INSTAGRAM_URL=https://instagram.com/ajcskillhub

# Contact Information
VITE_CONTACT_EMAIL=support@ajc-skill-hub.com
VITE_CONTACT_PHONE=+91-XXXXXXXXXX
VITE_SUPPORT_HOURS=9:00 AM - 6:00 PM IST

# Security
VITE_ENABLE_CSP=true
VITE_ENABLE_HSTS=true
VITE_SESSION_TIMEOUT=86400000

# Performance
VITE_ENABLE_SERVICE_WORKER=true
VITE_ENABLE_COMPRESSION=true
VITE_CACHE_DURATION=3600000

# Build Configuration
VITE_BUILD_SOURCEMAP=false
VITE_BUILD_MINIFY=true
VITE_BUILD_TARGET=esnext
